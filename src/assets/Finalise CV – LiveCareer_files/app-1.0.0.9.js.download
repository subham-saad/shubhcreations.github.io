RDL.GetNewText = function (editorText, tempText) {
    var div = $('<div>');
    var con = $(editorText);
    div.html(con);
    var innerText = div.text();
    if (innerText != tempText) {
        $(div.children()[0]).text(tempText);
        editorText = div.html();
    }
    return editorText;
}

