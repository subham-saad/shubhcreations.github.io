  if (navigator.userAgent.match(/iPad/i)) {
    _isIPAD = true;
  }
  else {
    _isIPAD = navigator.maxTouchPoints && navigator.maxTouchPoints > 2 && /MacIntel/.test(navigator.platform)
  }
  return !!_isIPAD;
}

